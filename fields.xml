					<Subfields>
						<Subfield>
							<Name>Additional Information</Name>
							<Format>30x</Format>
							<Prefix>/</Prefix>
							<Min>0</Min>
							<Max>1</Max>
						</Subfield>
					</Subfields>
				</Option>
				<Option id="RTGS">RTGS Payment</Option>
				<Option id="URGP">Urgent Payment</Option>
				</Options>
			</Subfield>
		</Subfields>
	</Field>
	<Field id="32B">
		<Name>Currency/Transaction Amount</Name>
		<Sequence>B</Sequence>
		<Presence>Mandatory</Presence>
		<Definition>This field specifies the currency and the amount of the subsequent transfer to be executed by the Receiver.</Definition>
		<Usage>The amount is subject to deduction of the Receiver's/beneficiary bank's charges if field 71A is BEN or SHA.</Usage>
		<Subfields>
			<Currency type="ISO3" />
			<Subfield>
				<Name>Amount</Name>
				<Format>15d</Format>
