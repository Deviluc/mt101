							<Subfield>
								<Name>Additional Information</Name>
								<Format>30x</Format>
								<Prefix>/</Prefix>
								<Min>0</Min>
								<Max>1</Max>
							</Subfield>
						</Subfields>
					</Option>
					<Option id="CMZB">Zero balance the account</Option>
					<Option id="CORT">Corporate Trade</Option>
					<Option id="EQUI">Equivalent Amount</Option>
					<Option id="INTC">Intra-Company Payment</Option>
					<Option id="NETS">Net Settlement System</Option>
					<Option id="OTHR">
						<Name>Other</Name>
						<Subfields>
							<Subfield>
								<Name>Additional Information</Name>
								<Format>30x</Format>
								<Prefix>/</Prefix>
								<Min>0</Min>
								<Max>1</Max>
							</Subfield>
						</Subfields>
					</Option>
